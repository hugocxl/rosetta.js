{
  "name": "@rosetta.js/formatter",
  "description": "Code formatter for Rosetta.js",
  "version": "0.1.2",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "author": "Hugo Corta <corta.hugo@gmail.com>",
  "scripts": {
    "dev": "pnpm build-fast --watch",
    "build": "tsup",
    "build-fast": "tsup --format=esm,cjs --no-dts"
  },
  "dependencies": {
    "@rosetta.js/node": "workspace:*",
    "@rosetta.js/utils": "workspace:*"
  },
  "devDependencies": {
    "@rosetta.js/types": "workspace:*"
  }
}
